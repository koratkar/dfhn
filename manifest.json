{
  "name": "DFHN",
  "version": "2020.03.02.42",
  "description": "Distraction Free Hacker News",
  "content_scripts": [{
    "js": ["df.js"],
    "matches": ["https://news.ycombinator.com/*"]
  }],
  "permissions": ["tabs"],
  "manifest_version": 3,
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
  }
}
